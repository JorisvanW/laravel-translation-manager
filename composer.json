{
    "name": "jorisvanw/laravel-translation-manager",
    "description": "Manage Laravel Translations",
    "keywords": [
        "laravel",
        "translations",
        "translator"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        },
        {
            "name": "Joris v. Willigen",
            "email": "me@jorisvanw.me"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.0 | ^9.0 | ^10.0",
        "symfony/finder": "~5.0 | ~6.0"
    },
    "suggest": {
        "jorisvanw/deepl-laravel": "If you want to translate using Deepl API."
    },
    "autoload": {
        "psr-4": {
            "Barryvdh\\TranslationManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Barryvdh\\TranslationManager\\ManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
