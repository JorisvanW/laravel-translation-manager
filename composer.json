{
    "name": "jorisvanw/laravel-translation-manager",
    "description": "Manage Laravel Translations",
    "keywords": [
        "laravel",
        "translations",
        "translator"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        },
        {
            "name": "Joris v. Willigen",
            "email": "me@jorisvanw.me"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.0 - 5.8 | ^6.0 | ^7.0 | ^8.0",
        "illuminate/translation": "5.0 - 5.8 | ^6.0 | ^7.0 | ^8.0",
        "symfony/finder": "~2.3 | ~3.0 | ~4.0 | ~5.0"
    },
    "suggest": {
        "jorisvanw/deepl-laravel": "Use DeepL Connect (OAuth) to authenticate via Laravel Socialite with the Mollie API. This is needed for some endpoints."
    },
    "autoload": {
        "psr-4": {
            "Barryvdh\\TranslationManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Barryvdh\\TranslationManager\\ManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
